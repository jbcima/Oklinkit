    <body id="want">
  
      <div id="activity">
        <div id="inside">
          <p>activity</p>
          <div class="badge"><p>1</p></div>
        </div>
      </div>
      <div id="activity-feed">
        <ul class="toggle">
          <li class="active" id="responses"><h4>Responses</h4></li>
          <li id="requests"><h4>Requests</h4></li>
        </ul>
        <p class="intro">people who are interested:</p>
        <ul class="feed" id="feed-responses">
          <!-- <div class="price"><h3><span>$</span><b>52</b><span>$</span></h3></div> -->
